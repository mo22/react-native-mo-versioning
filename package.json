{
  "name": "react-native-mo-versioning",
  "version": "0.1.13",
  "description": "react-native automatically set ios and android version by package.json",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "src/*",
    "lib/*",
    "android",
    "ios/**/*.{h,m,pbxproj}",
    "*.{podspec,gradle,sh,js}"
  ],
  "devDependencies": {
    "@types/react-native": "^0.67.3",
    "react": "^18.0.0",
    "react-native": "^0.68.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mo22/react-native-mo-versioning.git"
  },
  "author": "Moritz Moeller <mm@mxs.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mo22/react-native-mo-versioning/issues"
  },
  "homepage": "https://github.com/mo22/react-native-mo-versioning#readme",
  "scripts": {
    "postinstall": "echo remember to run node node_modules/react-native-mo-versioning/link.js postlink 1>&2"
  },
  "dependencies": {
    "@types/node": "^17.0.35",
    "@types/plist": "^3.0.2",
    "plist": "^3.0.5"
  }
}
